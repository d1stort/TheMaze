<Window x:Class="TheMaze.View.AccountWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TheMaze.View"
        mc:Ignorable="d"
        Title="AccountWindow" Height="160.8" Width="411.6">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0" Margin="0,10,0.2,0.2">
            <TextBlock Margin="80,5,46,5" Height="43" FontSize="22" HorizontalAlignment="Center" Width="250" RenderTransformOrigin="0.498,0.549">Do you have an account?</TextBlock>
        </StackPanel>
        <Button Grid.Row="1" Name="YesButtonAc" Height="30" Margin="92,10,220,24" Click="YesButtonAc_Click">Yes</Button>
        <Button Grid.Row="1" Name="NoButtonAc" Height="30" Margin="228,10,84,24" Click="NoButtonAc_Click">No</Button>
    </Grid>
</Window>
